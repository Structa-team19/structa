{
  "manifest_version": 3,
  "name": "Structa",
  "description": "__MSG_description__",
  "version": "1.0",
  "icons": {
    "128": "./structa-logo.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "./structa-logo.png",
    "default_title": "Structa"
  },
  "background": {
    "service_worker": "./service-worker.js",
    "type": "module"
  },
  "default_locale": "es",
  "permissions": ["activeTab", "scripting", "contextMenus", "tabs"]
}
